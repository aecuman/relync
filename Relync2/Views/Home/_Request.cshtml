@model Relync2.Models.Request


<br />
<div class="media"> 
    <div class="media-left"> 
    
    <img class="media-object" style="width: 64px; height: 64px;" alt="64x64" src="data:image/svg+xml;base64," data-src="holder.js/64x64" data-holder-rendered="true"> 
      
@Html.DisplayNameFor(model => model.Active): @Html.DisplayFor(model => model.Active)
        </div> 
    <div class="media-body"> 
    <h4 class="media-heading">@Html.DisplayFor(model => model.Ttype) | @Html.DisplayNameFor(model => model.Ptype): @Html.DisplayFor(model => model.Ptype) | @Html.DisplayNameFor(model => model.Location): @Html.DisplayFor(model => model.Location)</h4>
        
          
        
            @Html.DisplayNameFor(model => model.Highest): @Html.DisplayFor(model => model.Currency) @Model.Highest.Value.ToString("#,##0") |
            @Html.DisplayNameFor(model => model.Lowest): @Html.DisplayFor(model => model.Currency) @Model.Lowest.Value.ToString("#,##0")
        
    <br>
    @Html.DisplayNameFor(model => model.Attributes): @Html.DisplayFor(model => model.Attributes)
        <br />Requested On <small>@Html.DisplayFor(model => model.Dated)</small>
    </div>  
</div>